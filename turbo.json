{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build": {
      "outputs": ["dist/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "build:ci": {
      "outputs": ["dist/**", ".next/**", "storybook-static/**"],
      "dependsOn": ["^build:ci"]
    },
    "lint": {
      "outputs": [],
      "dependsOn": []
    },
    "lint:ci": {
      "outputs": [],
      "dependsOn": []
    },
    "test": {
      "outputs": [".coverage/**"],
      "dependsOn": []
    },
    "test:ci": {
      "outputs": [".coverage/**"],
      "dependsOn": []
    }
  }
}
